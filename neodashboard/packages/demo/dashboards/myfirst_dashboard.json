{
  "layout": {
      "title": false,
      "customJS" : ["jquery.peity.js"],
      "widgets" : [
          {"row" : 1, "col" : 1, "width" : 4, "height" : 4, "widget" : "calendar",     "job" : "google-calendar",     "config": "calendar-holidays"},

          {"row" : 1, "col" : 4, "width" : 2, "height" : 3, "widget" : "image",        "job" : "instagram",           "config": "instagram"},
          {"row" : 4, "col" : 4, "width" : 2, "height" : 1, "widget" : "utilization",  "job" : "t2utilization",       "config": "utilization"}
      ]
  },

  "config" : {

    "calendar-holidays" : {
       "interval" : 60000,
       "calendarUrl" : "http://www.google.com/calendar/ical/neo.com_jm3ba4ahl77t0f47602r85uhuo%40group.calendar.google.com/public/basic.ics",
       "maxEntries" : 8,
       "widgetTitle" : "Neo Events"
     },

     "instagram" : {
        "interval" : 300000,	
        "widgetTitle" : "Neo Instagram",
        "tag" : "neoinnovate"
     },

     "utilization": {
        "interval" : 60000,
        "widgetTitle" : "Utilization"
     }

  }
}