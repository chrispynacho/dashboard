{
  "layout": {
      "title": false,
      "customJS" : ["jquery.peity.js"],
      "widgets" : [
          {"row" : 1, "col" : 1, "width" : 4, "height" : 4, "widget" : "calendar",     "job" : "google-calendar",     "config": "calendar-holidays"},
          {"row" : 1, "col" : 5, "width" : 2, "height" : 2, "widget" : "image",        "job" : "picture-of-the-day",  "config": "picture"},
          {"row" : 1, "col" : 5, "width" : 2, "height" : 2, "widget" : "utilization",  "job" : "t2utilization",       "config": "utilization"}
      ]
  },

  "config" : {

    "calendar-holidays" : {
       "interval" : 60000,
       "calendarUrl" : "http://www.google.com/calendar/ical/neo.com_jm3ba4ahl77t0f47602r85uhuo%40group.calendar.google.com/public/basic.ics",
       "maxEntries" : 9,
       "widgetTitle" : "Neo Events"
     },

     "picture" : {
        "interval" : 60000,
        "widgetTitle" : "Nat Geo Picture of the Day",
        "url" : "http://photography.nationalgeographic.com/photography/photo-of-the-day"
     },

     "utilization": {
        "interval" : 60000,
        "widgetTitle" : "Utilization"
     }

  }
}